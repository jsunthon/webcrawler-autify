  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Main - Single Url",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/src/main.js",
      "args": ["https://www.google.com"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Main - Many Urls",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/src/main.js",
      "args": ["https://www.google.com", "https://autify.com"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Main - MetaData",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/src/main.js",
      "args": ["--metadata", "https://www.google.com", "https://autify.com"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Main - Bad URL",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/src/main.js",
      "args": ["https://www.gokokoogle.com"]
    }
  ]
}